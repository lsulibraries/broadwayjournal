exercise_list =
  ## contains any sequence of items organized as a list. [3.7. Lists]
  element tei:list {
    ((exercise_model.divTop | exercise_model.global)*,
     ((exercise_item, exercise_model.global*)+
      | (exercise_headLabel?,
         exercise_headItem?,
         (exercise_label,
          exercise_model.global*,
          exercise_item,
          exercise_model.global*)+)),
     (exercise_model.divBottom, exercise_model.global*)*)
    >> sch:pattern [
         id = "exercise-list-gloss-list-must-have-labels-constraint-6"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:list[@type='gloss']"
           "\x{a}" ~
           "	              "
           sch:assert [
             test = "tei:label"
             'The content of a "gloss" list should include a sequence of one or more pairs of a label element followed by an item element'
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    exercise_att.global.attributes,
    exercise_att.sortable.attributes,
    exercise_att.typed.attribute.subtype,
    ## describes the nature of the items in the list.
    ## Suggested values include: 1] gloss; 2] index; 3] instructions; 4] litany; 5] syllogism
