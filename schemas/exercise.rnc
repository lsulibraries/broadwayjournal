     | exercise_model.gLike
     | exercise_model.castItemPart
     | exercise_model.phrase
     | exercise_model.global)*,
    exercise_att.global.attributes,
    ## characterizes the cast item.
    [ a:defaultValue = "role" ]
    attribute type {
      ## the item describes a single role.
      "role" | ## the item describes a list of non-speaking roles.
        "list"
    }?,
    empty
  }

exercise_role =
