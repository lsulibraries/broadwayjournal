exercise_tag =
  ## contains text of a complete start- or end-tag, possibly including attribute specifications, but excluding the opening and closing markup delimiter characters. [22. Documentation Elements]
  element tei:tag {
    text,
    exercise_att.global.attributes,
    ## indicates the type of XML tag intended
    attribute type {
      ## a start-tag, with delimiters < and > is intended
      "start" | ## an end-tag, with delimiters </ and > is intended
        "end" | ## a empty tag, with delimiters < and /> is intended
        "empty" | ## a pi (processing instruction), with delimiters <? and ?> is intended
        "pi" | ## a comment, with delimiters <!-- and --> is intended
        "comment" | ## a marked-section, with delimiters <[CDATA[ and ]]> is intended
        "ms"
    }?,
    ## supplies the name of the schema in which this tag is defined.
    ## Sample values include: 1] TEI(text encoding initiative) ; 2] DBK(docbook) ; 3] XX(unknown) ; 4] Schematron; 5] HTML
    [ a:defaultValue = "TEI" ]
    attribute scheme { exercise_data.enumerated }?,
    empty
  }

exercise_val =
  ## (value) contains a single attribute value. [22. Documentation Elements 22.4.5. Attribute List Specification]
  element tei:val { text, exercise_att.global.attributes, empty }
exercise_model.rdgLike = exercise_rdg
exercise_model.rdgPart =
  exercise_wit
  | exercise_witStart
  | exercise_witEnd
  | exercise_lacunaStart
  | exercise_lacunaEnd
exercise_att.rdgPart.attributes = exercise_att.rdgPart.attribute.wit
exercise_att.rdgPart.attribute.wit =
  ## (witness or witnesses) contains a space-delimited list of one or more sigla indicating the witnesses to this reading beginning or ending at this point.
  attribute wit {
    list { exercise_data.pointer+ }
  }?
exercise_att.witnessed.attributes = exercise_att.witnessed.attribute.wit
exercise_att.witnessed.attribute.wit =
  ## (witness or witnesses) contains a space-delimited list of one or more pointers indicating the witnesses which attest to a given reading.
  attribute wit {
    list { exercise_data.pointer+ }
  }?
exercise_att.textCritical.attributes =
  exercise_att.source.attributes,
  exercise_att.textCritical.attribute.type,
  exercise_att.textCritical.attribute.cause,
  exercise_att.textCritical.attribute.varSeq,
  exercise_att.textCritical.attribute.hand
exercise_att.textCritical.attribute.type =
  ## classifies the reading according to some useful typology.
  ## Sample values include: 1] substantive; 2] orthographic
  attribute type { exercise_data.enumerated }?
exercise_att.textCritical.attribute.cause =
  ## classifies the cause for the variant reading, according to any appropriate typology of possible origins.
  ## Sample values include: 1] homeoteleuton; 2] homeoarchy; 3] paleographicConfusion; 4] haplography; 5] dittography; 6] falseEmendation
  attribute cause { exercise_data.enumerated }?
exercise_att.textCritical.attribute.varSeq =
  ## (variant sequence) provides a number indicating the position of this reading in a sequence, when there is reason to presume a sequence to the variants. 
  attribute varSeq { exercise_data.count }?
exercise_att.textCritical.attribute.hand =
  ## indicates the hand responsible for a particular reading in the witness.
  attribute hand { exercise_data.pointer }?

exercise_app =
  ## (apparatus entry) contains one entry in a critical apparatus, with an optional lemma and usually one or more readings or notes on the relevant passage. [12.1.1. The Apparatus Entry]
  element tei:app {
    (exercise_model.global*,
     (exercise_lem,
      exercise_model.global*,
      (exercise_wit, exercise_model.global*)?)?,
     ((exercise_model.rdgLike,
       exercise_model.global*,
       (exercise_wit, exercise_model.global*)?)
      | (exercise_rdgGrp,
         exercise_model.global*,
         (exercise_wit, exercise_model.global*)?))*),
    exercise_att.global.attributes,
    ## classifies the variation contained in this element according to some convenient typology.
    attribute type { exercise_data.enumerated }?,
    ## identifies the beginning of the lemma in the base text.
    attribute from { exercise_data.pointer }?,
    ## identifies the endpoint of the lemma in the base text.
    attribute to { exercise_data.pointer }?,
    ## (location) indicates the location of the variation, when the location-referenced method of apparatus markup is used.
    attribute loc {
      list { exercise_data.word+ }
    }?,
    empty
  }

exercise_listApp =
  ## (list of apparatus entries) contains a list of apparatus entries. [12.2. Linking the Apparatus to the Text]
  element tei:listApp {
    (exercise_model.headLike*, (exercise_app | exercise_listApp)+),
    exercise_att.global.attributes,
    exercise_att.sortable.attributes,
    exercise_att.declarable.attributes,
    exercise_att.typed.attributes,
    empty
  }

exercise_lem =
  ## (lemma) contains the lemma, or base text, of a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:lem {
    (text
     | exercise_model.divLike
     | exercise_model.pLike
     | exercise_model.gLike
     | exercise_model.phrase
     | exercise_model.inter
     | exercise_model.global
     | exercise_lg
     | exercise_l
     | exercise_model.rdgPart)*,
    exercise_att.global.attributes,
    exercise_att.textCritical.attributes,
    exercise_att.witnessed.attributes,
    empty
  }

exercise_rdg =
  ## (reading) contains a single reading within a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:rdg {
    (text
     | exercise_model.divLike
     | exercise_model.pLike
     | exercise_model.gLike
     | exercise_model.phrase
     | exercise_model.inter
     | exercise_model.global
     | exercise_lg
     | exercise_l
     | exercise_model.rdgPart)*,
    exercise_att.global.attributes,
    exercise_att.textCritical.attributes,
    exercise_att.witnessed.attributes,
    empty
  }

exercise_rdgGrp =
  ## (reading group) within a textual variation, groups two or more readings perceived to have a genetic relationship or other affinity. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:rdgGrp {
    (((exercise_rdgGrp, exercise_wit?)
      | ((exercise_lem, exercise_wit?)?,
         (exercise_model.rdgLike, exercise_wit?))*)+)
    >> sch:pattern [
         id = "exercise-rdgGrp-only1lem-constraint-12"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:rdgGrp"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "count(tei:lem) < 2"
             "Only one <lem> element may appear within a <rdgGrp>"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    exercise_att.global.attributes,
    exercise_att.textCritical.attributes,
    empty
  }

exercise_witDetail =
  ## (witness detail) gives further information about a particular witness, or witnesses, to a particular reading. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:witDetail {
    exercise_macro.phraseSeq,
    exercise_att.global.attributes,
    exercise_att.placement.attributes,
    exercise_att.source.attributes,
    exercise_att.pointing.attributes,
    ## (witnesses) indicates the sigil or sigla identifying the witness or witnesses to which the detail refers.
    attribute wit {
      list { exercise_data.pointer+ }
    },
    ## describes the type of information given about the witness.
    attribute type { exercise_data.enumerated }?,
