exercise_m =
  ## (morpheme) represents a grammatical morpheme. [17.1. Linguistic Segment Categories]
  element tei:m {
    (text
     | exercise_model.gLike
     | exercise_model.hiLike
     | exercise_seg
     | exercise_m
     | exercise_c
     | exercise_model.global)*,
    exercise_att.global.attributes,
    exercise_att.segLike.attributes,
    exercise_att.typed.attributes,
    ## supplies the morpheme's base form.
    attribute baseForm { exercise_data.word }?,
    empty
  }

exercise_c =
  ## (character) represents a character. [17.1. Linguistic Segment Categories]
  element tei:c {
    exercise_macro.xtext,
    exercise_att.global.attributes,
    exercise_att.segLike.attributes,
    exercise_att.typed.attributes,
    empty
  }

exercise_pc =
  ## (punctuation character) contains a character or string of characters regarded as constituting a single punctuation mark. [17.1. Linguistic Segment Categories]
  element tei:pc {
    (text
     | exercise_model.gLike
     | exercise_c
     | exercise_model.pPart.edit)*,
    exercise_att.global.attributes,
    exercise_att.segLike.attributes,
    exercise_att.typed.attributes,
    ## indicates the extent to which this punctuation mark conventionally separates words or phrases
    attribute force {
      ## the punctuation mark is a word separator
      "strong" | ## the punctuation mark is not a word separator
        "weak" | ## the punctuation mark may or may not be a word separator
        "inter"
    }?,
    ## provides a name for the kind of unit delimited by this punctuation mark.
    attribute unit { exercise_data.enumerated }?,
    ## indicates whether this punctuation mark precedes or follows the unit it delimits.
    attribute pre { exercise_data.truthValue }?,
    empty
  }

exercise_span =
  ## associates an interpretative annotation directly with a span of text. [17.3. Spans and Interpretations]
  element tei:span {
    exercise_macro.phraseSeq.limited
    >> sch:pattern [
         id = "exercise-span-targetfrom-constraint-10"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@from and @target"
             "\x{a}" ~
             "Only one of the attributes @target and @from may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "exercise-span-targetto-constraint-11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to and @target"
             "\x{a}" ~
             "Only one of the attributes @target and @to may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "exercise-span-tonotfrom-constraint-12"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to and not(@from)"
             "\x{a}" ~
             "If @to is supplied on "
             sch:name [ ]
             ", @from must be supplied as well"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "exercise-span-tofrom-constraint-13"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "contains(normalize-space(@to),' ') or contains(normalize-space(@from),' ')"
             "\x{a}" ~
             "The attributes @to and @from on "
             sch:name [ ]
             " may each contain only a single value"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    exercise_att.global.attributes,
    exercise_att.interpLike.attributes,
    exercise_att.pointing.attributes,
    ## gives the identifier of the node which is the starting point of the span of text being annotated; if not accompanied by a to attribute, gives the identifier of the node of the entire span of text being annotated.
    attribute from { exercise_data.pointer }?,
    ## gives the identifier of the node which is the end-point of the span of text being annotated.
    attribute to { exercise_data.pointer }?,
    empty
  }

exercise_spanGrp =
  ## (span group) collects together span tags. [17.3. Spans and Interpretations]
  element tei:spanGrp {
    exercise_span*,
    exercise_att.global.attributes,
    exercise_att.interpLike.attributes,
    empty
  }

exercise_interp =
  ## (interpretation) summarizes a specific interpretative annotation which can be linked to a span of text. [17.3. Spans and Interpretations]
  element tei:interp {
    (text
     | exercise_model.gLike
     | exercise_model.descLike
     | exercise_model.certLike)*,
