         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ptr"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "attributes @target and @cRef may be supplied on "
             sch:name [ ]
             "."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    exercise_att.global.attributes,
    exercise_att.pointing.attributes,
    exercise_att.internetMedia.attributes,
    exercise_att.typed.attributes,
    exercise_att.declaring.attributes,
