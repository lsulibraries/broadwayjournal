  element tei:witStart {
    empty,
    exercise_att.global.attributes,
    exercise_att.rdgPart.attributes,
    empty
  }

exercise_witEnd =
  ## (fragmented witness end) indicates the end, or suspension, of the text of a fragmentary witness. [12.1.5. Fragmentary Witnesses]
  element tei:witEnd {
