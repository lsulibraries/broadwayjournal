    exercise_att.global.attributes,
    exercise_att.segLike.attributes,
    exercise_att.typed.attributes,
    empty
  }

exercise_phr =
  ## (phrase) represents a grammatical phrase. [17.1. Linguistic Segment Categories]
  element tei:phr {
    exercise_macro.phraseSeq,
