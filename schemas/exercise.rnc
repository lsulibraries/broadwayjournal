    empty
    >> sch:pattern [
         id = "exercise-link-linkTargets3-constraint-9"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:link"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "contains(normalize-space(@target),' ')"
             "You must supply at least two values for @target or  on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    exercise_att.global.attributes,
    exercise_att.pointing.attributes,
    exercise_att.typed.attributes,
    empty
  }

